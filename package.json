{
  "name": "react-layouter",
  "version": "1.1.1",
  "description": "A component to use Layouter.js in React",
  "author": "Daniel P Z <danielpz@outlook.com>",
  "homepage": "https://layouter.io",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dapize/react-layouter.git"
  },
  "bugs": {
    "url": "https://github.com/dapize/react-layouter/issues"
  },
  "keywords": [
    "layouter",
    "danielpz",
    "layout",
    "react-layouter",
    "dapize"
  ],
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "license": "MIT",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:typecheck": "vitest --typecheck",
    "lint": "npx eslint src",
    "lint:fix": "npx eslint --fix src",
    "format": "prettier --write ./src --config ./.prettierrc"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^14.2.1",
    "@types/node": "^20.11.16",
    "@types/react": "^18.2.54",
    "@types/react-dom": "^18.2.18",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "@vitest/coverage-v8": "^1.2.2",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.33.2",
    "jsdom": "^24.0.0",
    "layouter.js": "^1.9.1",
    "prettier": "^3.2.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "typescript": "^5.3.3",
    "vite": "^5.0.12",
    "vite-plugin-dts": "^3.7.2",
    "vitest": "^1.2.2"
  },
  "peerDependencies": {
    "layouter.js": "^1.9.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  }
}
