{
  "name": "react-layouter",
  "version": "1.1.0",
  "description": "A component to use Layouter.js in React",
  "author": "Daniel P Z <danielpz@outlook.com>",
  "homepage": "https://layouter.io",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dapize/react-layouter.git"
  },
  "bugs": {
    "url": "https://github.com/dapize/react-layouter/issues"
  },
  "keywords": [
    "layouter",
    "danielpz",
    "layout",
    "react-layouter",
    "dapize"
  ],
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "license": "MIT",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:typecheck": "vitest typecheck",
    "lint": "npx eslint src",
    "lint:fix": "npx eslint --fix src",
    "format": "prettier --write ./src --config ./.prettierrc"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.1.4",
    "@testing-library/react": "^14.0.0",
    "@types/node": "^20.8.10",
    "@types/react": "^18.2.34",
    "@types/react-dom": "^18.2.14",
    "@typescript-eslint/eslint-plugin": "^6.9.1",
    "@typescript-eslint/parser": "^6.9.1",
    "@vitest/coverage-v8": "^0.34.6",
    "eslint": "^8.52.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-react": "^7.33.2",
    "jsdom": "^22.1.0",
    "prettier": "^3.0.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "typescript": "^5.0.2",
    "vite": "^4.4.5",
    "vite-plugin-dts": "^3.6.3",
    "vitest": "^0.34.6",
    "layouter.js": "^1.9.1"
  },
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "layouter.js": "^1.9.1"
  },
  "dependencies": {
    "zustand": "^4.4.6"
  }
}
